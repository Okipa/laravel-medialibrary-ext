{
    "name": "okipa/laravel-medialibrary-ext",
    "description": "Extra features for spatie/laravel-medialibrary package.",
    "keywords": [
        "spatie",
        "okipa",
        "laravel-medialibrary",
        "laravel-medialibrary-extension",
        "extension",
        "media",
        "conversion",
        "images",
        "downloads",
        "cms",
        "laravel"
    ],
    "homepage": "https://github.com/okipa/laravel-medialibrary-ext",
    "license": "MIT",
    "authors": [
        {
            "name": "Arthur LORENT",
            "email": "arthur.lorent@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4",
        "illuminate/support": "^7.0||^8.0",
        "spatie/laravel-medialibrary": "^9.0",
        "symfony/mime": "^5.0"
    },
    "require-dev": {
        "nunomaduro/larastan": "^0.6",
        "orchestra/testbench": "^5.0||^6.0",
        "phpmd/phpmd": "^2.8",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Okipa\\MediaLibraryExt\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Okipa\\MediaLibraryExt\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpcbf",
            "vendor/bin/phpcs",
            "vendor/bin/phpmd src text phpmd.xml",
            "vendor/bin/phpstan analyse --error-format=table --memory-limit=2048M",
            "vendor/bin/phpunit"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Okipa\\MediaLibraryExt\\MediaLibraryExtServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
